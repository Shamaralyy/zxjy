@import "../../nvue.wxss";

.content {
		width: 100%;
		height: 100vh;
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
		background-color: rgb(205, 255, 254);
}
.logo {
		width: 140rpx;
		height: 190rpx;
		border-radius: 100%;
		margin-top: 10rpx;
}
.camera {
		width: 430rpx;
		height: 270rpx;
		background-color: black;
}
.btn {
		position: absolute;
		top: 150rpx;
		background-color: rgb(4, 234, 231);
		border: 1px solid black;
		width: 90rpx;
		font-size: 13rpx;
		border-radius: 0%;
}
.rk {
		left: 40rpx;
}
.sb {
		right: 40rpx;
}
.close {
		top: 260rpx;
		right: 40rpx;
}
